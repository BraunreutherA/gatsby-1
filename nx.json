{
  "npmScope": "nrwl",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "lisDir": "packages"
  },
  "projects": {
    "gatsby-plugin": {
      "tags": []
    },
    "gatsby-plugin-e2e": {
      "tags": [],
      "implicitDependencies": ["gatsby-plugin"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test"]
      }
    }
  }
}
